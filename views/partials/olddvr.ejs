<!-- Example row of columns -->
<div class="row">
    <div class="col-md-4">
       <div class="row">
         <div class="column">
             <ul>
                <% data.forEach(function(info) { %>
                   <li><%= info.event_name%> - <%= info.es_id%>
                       <img src=<%= info.url%>
                   </li>
                <% }); %>
             </ul>
          </div>
       </div>
    </div>
</div>
